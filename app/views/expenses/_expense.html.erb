<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Amount</th>
      <th>Category</th>
      <th>Paid By</th>
      <!-- Otros encabezados de columna si es necesario -->
    </tr>
  </thead>
  <tbody>
    <% @expenses.each do |expense| %>
      <tr>
        <td><%= expense.name %></td>
        <td><%= number_to_currency(expense.amount) %></td>
        <td><%= expense.category %></td>
        <td><%= expense.paid_by %></td>
        <!-- Otros datos de la fila si es necesario -->
      </tr>
    <% end %>
  </tbody>
</table>
